{
  "www": {
    "apache": {
      "label": "Apache 2.4 mpm_prefork w/ PHP-FPM",
      "short": "Apache 2.4",
      "options": {
        "drupal": {
          "^8.3": "phase2/apache-php:php71",
          "^8": "phase2/apache-php:php70",
          "*": "phase2/apache-php:php56"
        },
        "default": "phase2/apache-php:php71"
      }
    }
  },
  "build": {
    "default": {
      "label": "Default",
      "options": {
        "drupal": {
          "^8.3.0-beta1": "phase2/devtools-build:php71",
          "^8": "phase2/devtools-build:php70",
          "*": "phase2/devtools-build:php56"
        },
        "default": "phase2/devtools-build:php71"
      }
    }
  },
  "cache": {
    "memcache": {
      "label": "Memcache",
      "options": {
        "default": "phase2/memcache"
      }
    },
    "redis": {
      "label": "Redis",
      "options": {
        "default": "phase2/redis"
      }
    }
  },
  "db": {
    "mariadb": {
      "label": "MariaDB",
      "options": {
        "default": "phase2/mariadb"
      }
    }
  },
  "proxy": {
    "varnish": {
      "label": "Varnish",
      "options": {
        "default": "phase2/varnish:4.0"
      }
    }
  },
  "mail": {
    "mailhog": {
      "label": "MailHog",
      "options": {
        "default": "mailhog/mailhog"
      }
    }
  }
}
